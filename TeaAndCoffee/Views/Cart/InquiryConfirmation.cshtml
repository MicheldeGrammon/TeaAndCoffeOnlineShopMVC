@model TeaAndCoffee_Models.OrderHeader
<div>
    @if (Model != null)
    {
        <h1 class="text-info">Order Created</h1>
        <p>Order ID = @Model.Id has been created successfully!</p>
    }
    else
    {
        <h1 class="text-info">Inquiry Confirmation</h1>

        <p>We have recieved your inquiry someone from our team will contact shortly!</p>
    }
</div>
